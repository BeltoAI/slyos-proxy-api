<style>
  :root{--flame-size:84px;--flame-speed:3.2s;--flame-glow:rgba(255,184,0,.85);--flame-core:rgba(255,122,24,.98);--flame-mid:rgba(255,184,0,.92);--flame-tip:rgba(255,61,129,.92)}
  .burning-flame{position:relative;width:var(--flame-size);height:var(--flame-size);-webkit-mask-image:url('assets/flame.png');mask-image:url('assets/flame.png');-webkit-mask-size:contain;mask-size:contain;-webkit-mask-repeat:no-repeat;mask-repeat:no-repeat;-webkit-mask-position:center;mask-position:center;background:radial-gradient(55% 65% at 45% 60%, var(--flame-core) 0%, transparent 60%),conic-gradient(from 0deg, var(--flame-core), var(--flame-mid), var(--flame-tip), var(--flame-core));filter:drop-shadow(0 0 22px rgba(255,184,0,.55));animation:flameHue var(--flame-speed) cubic-bezier(.22,.61,.36,1) infinite,flamePulse calc(var(--flame-speed)*.82) ease-in-out infinite,flameLick 1.15s steps(2,end) infinite}
  .burning-flame::before,.burning-flame::after{content:"";position:absolute;inset:0;-webkit-mask-image:inherit;mask-image:inherit;-webkit-mask-size:inherit;mask-size:inherit;-webkit-mask-repeat:no-repeat;mask-repeat:no-repeat;-webkit-mask-position:center;mask-position:center;pointer-events:none}
  .burning-flame::before{background:radial-gradient(40% 45% at 50% 64%, rgba(255,255,255,.65), transparent 60%),radial-gradient(70% 80% at 50% 70%, rgba(255,184,0,.35), transparent 70%);mix-blend-mode:screen;animation: corePulse 1.6s ease-in-out infinite}
  .burning-flame::after{background:radial-gradient(8% 10% at 55% 35%, rgba(255,255,255,.25), transparent 70%),radial-gradient(6% 8% at 48% 55%, rgba(255,255,255,.18), transparent 70%),radial-gradient(5% 7% at 60% 70%, rgba(255,255,255,.15), transparent 70%);filter: blur(.4px);animation: ember 1.8s linear infinite}
  @keyframes flameHue{0%{filter:hue-rotate(0deg) drop-shadow(0 0 14px rgba(255,184,0,.65))}45%{filter:hue-rotate(18deg) drop-shadow(0 0 36px rgba(255,184,0,.95))}100%{filter:hue-rotate(0deg) drop-shadow(0 0 14px rgba(255,184,0,.65))}}
  @keyframes flamePulse{0%,100%{transform:translateY(0) scaleY(1) scaleX(1)}50%{transform:translateY(-1px) scaleY(1.06) scaleX(.985)}}
  @keyframes flameLick{0%{background-position:0 0,0 0}50%{background-position:0 1px,2px -1px}100%{background-position:0 0,0 0}}
  @keyframes corePulse{0%,100%{opacity:.85;transform:scale(1)}50%{opacity:1;transform:scale(1.06)}}
  @keyframes ember{0%{opacity:.35;transform:translateY(0) scale(.98)}50%{opacity:.55;transform:translateY(-1px) scale(1.02)}100%{opacity:.35;transform:translateY(0) scale(.98)}}
  @media (prefers-reduced-motion:reduce){.burning-flame,.burning-flame::before,.burning-flame::after{animation:none}}
</style>
  </footer>

  <script>document.getElementById('year').textContent=new Date().getFullYear();</script>
  <script src="app.js"></script>
</body>
</html>
