  </style>

  <script>
    (function(){
      const nav   = document.querySelector('.nav.navx');
      const btn   = nav.querySelector('.navx-toggle');
      const panel = nav.querySelector('#navx-panel');
      const scrim = nav.querySelector('.navx-scrim');
      const close = nav.querySelector('.navx-close');
      function open(){ nav.dataset.open='true'; btn.setAttribute('aria-expanded','true'); panel.hidden=false; scrim.hidden=false; document.body.classList.add('navx-lock'); const first=panel.querySelector('a,button'); if(first) setTimeout(()=>first.focus({preventScroll:true}), 10) }
      function closeMenu(){ nav.dataset.open='false'; btn.setAttribute('aria-expanded','false'); document.body.classList.remove('navx-lock'); setTimeout(()=>{ panel.hidden=true; scrim.hidden=true; },300); btn.focus({preventScroll:true}) }
      btn.addEventListener('click', ()=> (nav.dataset.open==='true'?closeMenu():open()));
      close.addEventListener('click', closeMenu);
      scrim.addEventListener('click', closeMenu);
      panel.addEventListener('click', e=>{ if(e.target.matches('a')) closeMenu(); });
      document.addEventListener('keydown', e=>{ if(e.key==='Escape') closeMenu(); });
      const mq = window.matchMedia('(min-width: 861px)');
      mq.addEventListener('change', () => { if(mq.matches) closeMenu(); });
    })();
  </script>
